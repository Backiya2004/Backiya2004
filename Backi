def
linear_search_product(product_
list, target_product):
    indices = []
    for i, product in
enumerate(product_list):
        if product ==
target_product:
            indices.append(i)
    return indices
products = ["Apple", "Banana",
"orange", "Apple", "Grapes"]
target = "Apple"
result =
linear_search_product(products
, target)
print(result)  # output: [0,
3]
