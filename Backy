class student:
    def __init__(self, name,
roll_number, cgps):
        self.name = name
        self.roll_number _
roll_number
        self.cgpa = cgpa

def
sort_students(student_list):
    sorted_students =
sorted(student_list,
key=lambda student:
student.cgpa, reverse=True)
    return sorted_students
# Create a list of Student
objects
students = [
    Student("Alice", "A001",
3.8),
    Student("Bob", "B002",
3.6),
    Student("Charlie", "C003",
3.9),
    Student("David", "D004",
3.7),
]

# Sort the Students by CGPA
sorted_studebts =
sort_students(students)

# Print the sorted list
for student in
sorted_students:
    print(f"Nsme:
{student.name}, Roll Number:
{student.roll_mumber}, CGPA:
{student.cgpa}")







